    "indonesia",
    "indonesian-language",
    "translation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Budi Hermawan",
      "email": "budi03@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/realodix/flarum-ext-indonesian/issues",
